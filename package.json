{
  "name": "pet-hero",
  "version": "0.0.1",
  "description": "Pet Hero is an open-source application enabling people to find pets to adopt.",
  "keywords": [
    "animal",
    "animals",
    "pet",
    "pets",
    "adopt",
    "adoption",
    "animal shelter",
    "animal shelters"
  ],
  "author": "Christopher Gates <topher.gates@gmail.com> (https://github.com/tophergates)",
  "contributors": [
    "Christopher Gates <topher.gates@gmail.com> (https://github.com/tophergates)",
    "Ryan C. Martin (https://github.com/ryancmartin)",
    "Baltazar Nunez (https://github.com/baltz1)",
    "Michael Pimental Jr (https://github.com/mtpjr88)"
  ],
  "main": "index.js",
  "engines": {
    "node": ">=8.9.4"
  },
  "scripts": {
    "start": "node index.js",
    "dev": "nodemon index.js",
    "eslint": "eslint *.js **/*.js --ignore-pattern node_modules/",
    "mocha": "mocha tests --recursive",
    "test": "cross-env NODE_ENV=testing concurrently \"npm run eslint\" \"npm run mocha\"",
    "test-watch": "nodemon --exec 'npm run test'"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/pet-heroes/pet-hero.git"
  },
  "bugs": {
    "url": "https://github.com/pet-heroes/pet-hero/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "eslint": "^4.17.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-mocha": "^4.11.0",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.6.1",
    "mocha": "^5.0.1",
    "nodemon": "^1.14.12",
    "prettier": "^1.10.2",
    "supertest": "^3.0.0"
  },
  "dependencies": {
    "body-parser": "^1.18.2",
    "concurrently": "^3.5.1",
    "cors": "^2.8.4",
    "cross-env": "^5.1.3",
    "express": "^4.16.2",
    "helmet": "^3.10.0",
    "mongoose": "^5.0.5",
    "morgan": "^1.9.0"
  }
}
